    "@storybook/react": "^3.4.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.0.0",
    "preact-compat": "^3.18.0"
  },
  "dependencies": {
    "preact": "^8.2.1"
  }
